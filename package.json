{
   "name": "redash-proxy",
   "version": "1.0.0",
   "description": "Redash api proxy",
   "main": "app.js",
   "scripts": {
      "build": "tsc",
      "start": "node ./dist/server.js",
      "clean": "rm -rf ./dist && mkdir dist",
      "dev": "nodemon --config nodemon.json src/server.ts",
      "dev:debug": "nodemon --config nodemon.json --inspect-brk src/server.ts",
      "test": "jest",
      "test:watch": "jest --watchAll",
      "lint": "ts-standard",
      "lint:fix": "ts-standard --fix",
      "prepare": "husky install",
      "migrations:generate": "npx ts-node ./node_modules/.bin/typeorm migration:generate -f ./src/config/database.ts",
      "migrations:run": "npx ts-node ./node_modules/.bin/typeorm migration:run -f ./src/config/database.ts",
      "migrations:revert": "npx ts-node ./node_modules/.bin/typeorm migration:revert -f ./src/config/database.ts"
   },
   "keywords": [
      "Sovryn"
   ],
   "author": "Remy",
   "license": "ISC",
   "dependencies": {
      "axios": "^0.21.4",
      "class-validator": "^0.13.2",
      "convict": "^5.2.1",
      "cors": "^2.8.5",
      "express": "^4.18.1",
      "express-request-id": "^1.4.1",
      "express-validator": "^6.14.1",
      "helmet": "^4.6.0",
      "lodash": "^4.17.21",
      "moment": "^2.29.3",
      "pg": "^8.7.3",
      "pino": "^6.14.0",
      "pino-http": "^5.8.0",
      "pino-pretty": "^5.1.3",
      "reflect-metadata": "^0.1.13",
      "regenerator-runtime": "^0.13.7",
      "response-time": "^2.3.2",
      "typeorm": "^0.2.45",
      "uuid": "^8.3.1"
   },
   "devDependencies": {
      "@tsconfig/node12": "^1.0.9",
      "@types/bunyan": "^1.8.8",
      "@types/convict": "^6.1.1",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/express-request-id": "^1.4.3",
      "@types/jest": "^27.5.1",
      "@types/lodash": "^4.14.182",
      "@types/node": "^8.10.66",
      "@types/pino": "^6.3.12",
      "@types/pino-http": "^5.8.1",
      "@types/response-time": "^2.3.5",
      "@types/supertest": "^2.0.12",
      "husky": "^7.0.4",
      "jest": "^27.5.1",
      "nodemon": "^2.0.16",
      "supertest": "^6.2.3",
      "ts-jest": "^27.1.5",
      "ts-node": "^10.8.0",
      "ts-standard": "^10.0.0",
      "typescript": "^4.7.2"
   }
}